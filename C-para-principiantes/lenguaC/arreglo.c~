 #include<stdio.h>

     main()

     {

      int i,j,n[500],aux,k;

      printf(" program coyote1.\n");

      printf("INTRODUZCA LA CANTIDAD DE NUMEROS (MAXIMO DE 500):?\n");

      scanf("%d",&k);

      printf("INTRODUZCA LOS NUMEROS A ARREGLAR:\n");

      for (i=0;i<k;++i)

      {
          printf("n[%d",i);
          printf("n[%d",i);

          printf("] =");

          scanf("%d",&n[i]);

       }
       printf("ARREGLO DESEADO:\n");

       for (i=0;i<k-1;i++)

       for (j=i+1;j<k;++j)

       if (n[i]>n[j])

       {

         aux=n[i];
         n[i]=n[j];
         n[j]=aux;

       }     

       for (i=0;i<k;++i)

        printf(":%d\n",n[i]);

       }
                                 